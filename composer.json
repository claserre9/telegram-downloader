{
    "name": "claserre9/telegram-downloader",
    "type": "project",
    "description": "Telegram Downloader is a tool designed to simplify the process of downloading media from Telegram channels",
    "license": "MIT",
    "authors": [
        {
            "name": "Clifford Laserre",
            "email": "claserre9@gmail.com",
            "homepage": "https://claserre9.github.io"
        }
    ],
    "require": {
        "php" : ">=8.2",
        "vlucas/phpdotenv": "^5.6",
        "danog/madelineproto": "^8",
        "symfony/filesystem": "^7.2",
        "symfony/console": "^7.2",
        "slim/slim": "^4.14",
        "slim/psr7": "^1.6"
    },
    "config": {
        "preferred-install": "source",
        "archive-format": "zip"
    },
    "autoload": {
        "psr-4": {
            "claserre9\\" : "src"
        }
    },
    "minimum-stability": "beta",
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpstan/phpstan": "^2.1"
    },
    "scripts": {
        "stan" : "php vendor/bin/phpstan analyse src --level 7 --memory-limit=-1"
    }
}
